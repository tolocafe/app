{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "common": {
      "bun": "1.2.19",
      "node": "22.18.0"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "extends": "common"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "extends": "common"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "extends": "common"
    }
  },
  "submit": {
    "production": {}
  }
}
